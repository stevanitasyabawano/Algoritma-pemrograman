s = int(input("Masukkan Angkanya : "))
t = int(input("Masukkan Angka untuk dipangkatkan: "))

def pangkat( s, t ):
    
    if t >= 1 :
        return s * pangkat(s, t  - 1)
    
    return s

hasil = pangkat( s, t )
print(f"hasilnya adalah = {hasil}")
