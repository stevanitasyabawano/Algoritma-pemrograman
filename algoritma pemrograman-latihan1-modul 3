import math

print("jenis segitiga berdasarkan panjang sisi ")
ta = float(input("masukan sisi a ="))
tb = float(input("masukan sisi b ="))
tc = float(input("masukan sisi c ="))


if(ta==tb==tc):
    print("merupakan segitiga sama sisi")
elif(ta==tb or tb==tc or tc==ta):
    print("merupakan segitiga sama kaki")  
else: 
  if((ta+tb <= tc) or (ta+tc <= tb) or (tb+tc <= ta)):
    print(" bukan merupakan segitiga")
  else:
    print("merupakan segitiga sembarang")    
